{{!-- Main --}}
      <section class="sheet-main">

        {{!-- Sheet Tab Navigation --}}
        <nav class="sheet-tabs tabs" data-group="primary">
          <a class="item" data-tab="description">{{localize "PBTA.Description"}}</a>
          <a class="item" data-tab="moves">{{localize "PBTA.Moves"}}</a>
          {{#unless isObserver}}
          <a class="item" data-tab="equipment">{{localize "PBTA.Equipment"}}</a>
          <a class="item" data-tab="Growth">{{localize "FWRPG-SHEETS.Growth"}}</a>
          {{/unless}}
        </nav>

        {{!-- Sheet Body --}}
        <section class="sheet-body">

          {{!-- Main Tab --}}
          <div class="tab description" data-group="primary" data-tab="description">
            <div class="sheet-tab">
              <div class="cell cell--bio">
                <label class="cell__title">{{localize "FWRPG-SHEETS.Blood"}}</label>
                {{editor system.attrTop.blood.value target="system.attrTop.blood.value" button=true owner=owner
                editable=editable}}
              </div>
              <div class="cell cell--bio">
                <label class="cell__title">{{localize "FWRPG-SHEETS.Kin"}}</label>
                {{editor system.attrTop.kin.value target="system.attrTop.kin.value" button=true owner=owner
                editable=editable}}
              </div>
              <div class="cell cell--bio">
                <label class="cell__title">{{localize "FWRPG-SHEETS.Look"}}</label>
                {{editor system.attrLeft.look target="system.attrLeft.look.value" button=true owner=owner
                editable=editable}}
              </div>
              {{#unless isObserver}}
              <div class="cell cell--bio">
                <label class="cell__title">{{localize "FWRPG-SHEETS.Issue"}}</label>
                {{editor system.attrTop.issue target="system.attrTop.issue.value" button=true owner=owner
                editable=editable}}
              </div>
              <div class="cell cell--bio">
                <label class="cell__title">{{localize "FWRPG-SHEETS.Doubt"}}</label>
                {{editor system.attrTop.look target="system.attrTop.look.value" button=true owner=owner
                editable=editable}}
              </div>
              {{/unless}}
            </div>
          </div>